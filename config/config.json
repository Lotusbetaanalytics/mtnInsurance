{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "mtninsurance-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mtninsurance/MtninsuranceWebPart.js",
          "manifest": "./src/webparts/mtninsurance/MtninsuranceWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",

      "globalName": "jQuery"
    },

    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",

      "globalName": "bootstrap",

      "globalDependencies": ["jquery"]
    }
  },
  "localizedResources": {
    "MtninsuranceWebPartStrings": "lib/webparts/mtninsurance/loc/{locale}.js"
  }
}
